s/Files/Projects/blog/src/index.js",[],"/Volumes/Files/Projects/blog/src/components/Posts.jsx",[],["101","102"],"/Volumes/Files/Projects/blog/src/firebase.js",[],"/Volumes/Files/Projects/blog/src/components/AddPost.jsx",[],["103","104"],"/Volumes/Files/Projects/blog/src/components/Post.jsx",["105"],"import {\n  Avatar,\n  Box,\n  Button,\n  Card,\n  CardContent,\n  Grid,\n  Typography,\n} from \"@material-ui/core\"\nimport { blue } from \"@material-ui/core/colors\"\nimport moment from \"moment\"\nimport React, { useContext } from \"react\"\nimport { Link } from \"react-router-dom\"\nimport { firestore } from \"../firebase\"\nimport { userContext } from \"../providers/UsersProvider\"\nimport { makeStyles } from \"@material-ui/core/styles\"\n\nconst useStyles = makeStyles((theme) => ({\n  card: {\n    backgroundColor: blue[600],\n  },\n}))\n\nconst Post = ({ id, title, content, comments, stars, createdAt, user }) => {\n  const classes = useStyles()\n  const postRef = firestore.doc(`posts/${id}`)\n\n  const remove = () => postRef.delete()\n\n  const star = () => postRef.update({ stars: stars + 1 })\n\n  const currentUser = useContext(userContext)\n  const checkCurrentUser = (currentUser, user) => {\n    if (currentUser) {\n      if (currentUser.uid === user.uid) {\n        return true\n      }\n    } else {\n      return false\n    }\n  }\n\n  return (\n    <Grid item>\n      <Card className={classes.card}>\n        <CardContent>\n          <Link to={`/posts/${id}`}>\n            <Typography variant=\"h5\">{title}</Typography>\n          </Link>\n          <Typography>{content}</Typography>\n          <Typography>Comments: {comments}</Typography>\n          <Typography>\n            Created At{moment(createdAt.toDate()).calendar()}\n          </Typography>\n          <Typography>⭐️{stars}</Typography>\n          <Typography>Created By{user.displayName}</Typography>\n          <Avatar src={user.photoURL} />\n          <Button onClick={star}>Star</Button>\n          {checkCurrentUser(currentUser, user) && (\n            <Button onClick={remove}>delete</Button>\n          )}\n        </CardContent>\n      </Card>\n    </Grid>\n  )\n}\n\nexport default Post\n","/Volumes/Files/Projects/blog/src/theme.js",[],"/Volumes/Files/Projects/blog/src/components/Authentication.jsx",["106"],"import { Box, Grid } from \"@material-ui/core\"\nimport React, { useContext } from \"react\"\nimport { userContext } from \"../providers/UsersProvider\"\nimport SignIn from \"./SignIn\"\nimport SingInAndSignUp from \"./SingInAndSignUp\"\nimport UserCard from \"./UserCard\"\n\nconst Authentication = () => {\n  const user = useContext(userContext)\n  console.log(user)\n  return (\n    <Box>\n      <Grid container>\n        <Grid item>{user ? <UserCard {...user} /> : <SignIn />}</Grid>\n        <Grid item></Grid>\n      </Grid>\n    </Box>\n  )\n}\n\nexport default Authentication\n","/Volumes/Files/Projects/blog/src/components/SignIn.jsx",["107","108"],"import { Box, Button, Grid, TextField, Typography } from \"@material-ui/core\"\nimport React, { useState } from \"react\"\nimport { auth, signInWithGoogle } from \"../firebase\"\nimport { makeStyles } from \"@material-ui/core/styles\"\nimport google from \"../images/btn_google_dark_normal_ios.svg\"\n\nconst useStyles = makeStyles((theme) => ({\n  button: {\n    backgroundColor: theme.palette.warning.main,\n  },\n}))\n\nconst initialState = { email: \"\", password: \"\" }\n\nconst SignIn = () => {\n  const classes = useStyles()\n  const [state, setState] = useState(initialState)\n\n  const handleChange = (event) => {\n    const { name, value } = event.target\n    setState({ ...state, [name]: value })\n  }\n\n  const handleSubmit = (event) => {\n    event.preventDefault()\n\n    auth\n      .signInWithEmailAndPassword(state.email, state.password)\n      .catch((error) => console.error(error))\n\n    setState(initialState)\n  }\n  return (\n    <Box>\n      <Grid container spacing={2}>\n        <Grid item container xs={6} alignItems=\"center\">\n          <Button\n            onClick={signInWithGoogle}\n            variant=\"contained\"\n            className={classes.button}\n          >\n            Sign in with <img src={google} />\n          </Button>\n        </Grid>\n        <Grid item container xs={6} justify=\"center\" spacing={1}>\n          <Grid item>\n            <TextField\n              variant=\"outlined\"\n              name=\"email\"\n              placeholder=\"Email\"\n              value={state.email}\n              onChange={handleChange}\n            />\n          </Grid>\n          <Grid item>\n            <TextField\n              variant=\"outlined\"\n              type=\"password\"\n              name=\"password\"\n              placeholder=\"password\"\n              value={state.password}\n              onChange={handleChange}\n            />\n          </Grid>\n          <Grid item>\n            <Button\n              onClick={handleSubmit}\n              variant=\"contained\"\n              className={classes.button}\n            >\n              Sign In\n            </Button>\n          </Grid>\n        </Grid>\n      </Grid>\n    </Box>\n  )\n}\n\nexport default SignIn\n","/Volumes/Files/Projects/blog/src/components/UserCard.jsx",["109","110","111"],"/Volumes/Files/Projects/blog/src/utilities.js",[],"/Volumes/Files/Projects/blog/src/providers/UsersProvider.jsx",[],["112","113"],"/Volumes/Files/Projects/blog/src/providers/PostsProvider.jsx",[],["114","115"],"/Volumes/Files/Projects/blog/src/components/SignUp.jsx",[],"/Volumes/Files/Projects/blog/src/components/UserProfile.jsx",[],"/Volumes/Files/Projects/blog/src/components/PostPage.jsx",["116","117"],"import { Box } from \"@material-ui/core\"\nimport React, { useEffect, useState } from \"react\"\nimport { withRouter } from \"react-router-dom\"\nimport { firestore } from \"../firebase\"\nimport { docsWithId } from \"../utilities\"\nimport addUserContext from \"./addUserContext\"\nimport Comments 